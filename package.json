    "react-dom": "^18.2.0",
    "sass": "^1.56.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "nebula-genesis": "^1.2.2",
    "typescript": "^4.9.4",
    "webpack-cli": "^4.10.0"
  }
