{
  "name": "nebula-genesis",
  "version": "1.4.3",
  "private": true,
  "description": "POC site for Nebula Genesis static website product",
  "author": "RIMAROK.com",
  "keywords": [
    "creative",
    "blog",
    "static",
    "jamstack",
    "gatsby"
  ],
  "scripts": {
